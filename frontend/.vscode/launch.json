  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "attach",
      "name": "Attach to Parcel",
      "port": 9222,
      "webRoot": "${workspaceFolder}",
      "url": "http://localhost:1234", // Update with your Parcel development server port
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/*"
      }
    }
  ]
}
